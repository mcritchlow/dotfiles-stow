Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa_work

Host lib-hydrahead-prod.ucsd.edu
  HostKeyAlgorithms +ssh-rsa
  PubkeyAcceptedKeyTypes +ssh-rsa
  IdentityFile ~/.ssh/id_rsa_work

Host lib-hydrahead-staging.ucsd.edu
  HostKeyAlgorithms +ssh-rsa
  PubkeyAcceptedKeyTypes +ssh-rsa
  IdentityFile ~/.ssh/id_rsa_work

Host lib-hydrahead-qa.ucsd.edu
  HostKeyAlgorithms +ssh-rsa
  PubkeyAcceptedKeyTypes +ssh-rsa
  IdentityFile ~/.ssh/id_rsa_work

Host gitlab.com
  HostName gitlab.com
  User git
  IdentityFile ~/.ssh/id_rsa_work

AddKeysToAgent yes
